RewriteEngine On
RewriteBase /

RewriteCond %{HTTP_HOST} !^www\.oopzy\.com$ [NC]
RewriteCond %{SERVER_PORT} !^443$ 
RewriteRule ^(.*)$ http://www.oopzy.com/$1 [R=301,L]

RewriteCond $1 !^(index\.php|favicon\.ico|assets|_mmServerScripts|robots\.txt|healthcheck\.php)
RewriteRule ^(.*)$ /index.php/$1 [L]